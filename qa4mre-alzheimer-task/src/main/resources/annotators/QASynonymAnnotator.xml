      <configurationParameter>
        <name>FILE_NAME</name>
        <description>synonym file</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>GIGA_WORD</name>
